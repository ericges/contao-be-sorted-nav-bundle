{
  "name": "ericges/contao-be-sorted-nav-bundle",
  "description": "Add this to your contao project to have an alphabetically sorted backend navigation. This is useful if you have a lot of backend modules and want to find them quickly.",
  "keywords": [
    "contao",
    "backend",
    "navigation",
    "menu",
    "alphabetical",
    "sort",
    "sorting"
  ],
  "type": "contao-bundle",
  "homepage": "https://github.com/ericges/contao-be-sorted-nav-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Eric Gesemann",
      "email": "eric.ges.dev@gmail.com",
      "homepage": "https://ges.dev",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.4 || ^8.0",
    "contao/core-bundle": "^4.8 || ^5.0",
    "symfony/http-foundation": "^5.4 || ^6.0"
  },
  "autoload": {
    "psr-4": {
      "GES\\Contao\\BESortedNavBundle\\": "src/"
    }
  },
  "extra": {
    "contao-manager-plugin": "GES\\Contao\\BESortedNavBundle\\ContaoManager\\Plugin"
  }
}